{
  "asset_id": "takumi-use-case-video-001",
  "asset_version": "1.0.0",
  "display_name": "AI Video Generation (AnimateDiff)",
  "description": "Create smooth AI animations using AnimateDiff Evolved and VideoHelperSuite.",
  "environment": {
    "name": "animatediff_env",
    "engine": "conda",
    "components": [
      { "type": "conda", "source": "python", "version": "3.10" },
      { "type": "conda", "source": "pip" },
      { "type": "conda", "source": "pytorch-cuda", "version": "12.1", "channel": "pytorch" },
      { "type": "conda", "source": "pytorch", "version": "2.1.2", "channel": "pytorch" },
      { "type": "conda", "source": "torchvision", "version": "0.16.2", "channel": "pytorch" },
      { "type": "conda", "source": "torchaudio", "version": "2.1.2", "channel": "pytorch" },
      { "type": "conda", "source": "cuda-libraries", "version": "12.1", "channel": "nvidia" },
      { "type": "conda", "source": "ffmpeg", "version": ">=6.0", "channel": "conda-forge" }
    ]
  },
  "components": [
    {
      "type": "git-clone",
      "source": "https://github.com/comfyanonymous/ComfyUI.git",
      "version": "master",
      "path": "/app/ComfyUI"
    },
    {
      "type": "custom-node",
      "source": "ComfyUI-AnimateDiff-Evolved",
      "version": "main"
    },
    {
      "type": "custom-node",
      "source": "ComfyUI-VideoHelperSuite",
      "version": "main"
    },
    {
      "type": "custom-node",
      "source": "ComfyUI-Advanced-ControlNet",
      "version": "main"
    },
    {
      "type": "custom-node",
      "source": "ComfyUI-Manager",
      "version": "main"
    },
    { "type": "pip", "source": "tqdm" },
    { "type": "pip", "source": "imageio[ffmpeg]" },
    { "type": "pip", "source": "numpy", "version": "<2" },
    { "type": "pip", "source": "aiohttp" },
    { "type": "pip", "source": "safetensors" },
    { "type": "pip", "source": "pyyaml" },
    { "type": "pip", "source": "packaging" },
    { "type": "pip", "source": "pillow" },
    { "type": "pip", "source": "requests" },
    { "type": "pip", "source": "psutil" },
    { "type": "pip", "source": "huggingface_hub" },
