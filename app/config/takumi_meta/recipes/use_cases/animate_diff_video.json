    { "type": "pip", "source": "soundfile" },
    { "type": "pip", "source": "av" },
    { "type": "pip", "source": "opencv-python" },
    { "type": "pip", "source": "scipy" },
    { "type": "pip", "source": "filelock" },
    { "type": "pip", "source": "gitpython" },
    { "type": "pip", "source": "matrix-client" },
    { "type": "pip", "source": "pydantic" },
    { "type": "pip", "source": "pydantic-settings" },
